{
  "extends": "tslint:latest",
  "rules": {
    "callable-types": true,
    "interface-name": [true],
    "interface-over-type-literal": true,
    "file-header": [
      false
    ],
    "semicolon": [
      false
    ],
    "quotemark": [
      false
    ],
    "max-line-length": [
      true,
      140
    ],
    "cyclomatic-complexity": [
      true
    ],
    "member-ordering": true,
    "no-angle-bracket-type-assertion": true,
    "no-default-export": false,
    "no-empty-interface": true,
    "no-unused-new": false,
    "eofline": false,
    "no-string-throw": true,
    "prefer-const": true,
    "no-unused-expression": false,
    "trailing-comma": [
      false
    ],
    "no-namespace": [
      false
    ],
    "array-type": [true, "generic"],
    "arrow-parens": [false],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ]
  }
}